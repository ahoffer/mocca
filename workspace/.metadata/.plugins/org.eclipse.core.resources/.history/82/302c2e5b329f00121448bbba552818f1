package weka.subspaceClusterer;

public class MySubspaceClusterer extends SubspaceClusterEvaluation {

	  public static void main (String[] args) {
		    try {
		      if (args.length == 0) {
			throw  new Exception("The first argument must be the name of a " 
					     + "clusterer");
		      }

		      String ClustererString = args[0];
		      args[0] = "";
		      MySubspaceClusterer newClusterer = MySubspaceClusterer.forName(ClustererString, null);
		      System.out.println(evaluateClusterer(newClusterer, args));
		    }
		    catch (Exception e) {
		      System.out.println(e.getMessage());
		    }
		  }

}
